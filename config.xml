<?xml version='1.0' encoding='utf-8'?>
<widget id="link.tes8.www" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Lifestyle Clinic</name>
    <description>
        A Lifestyle Clinic online patient management system.
    </description>
    <author email="phlar89@gmail.com" href="https://www.tes8.link">
        Ralph Palomar
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
	<preference name="android-minSdkVersion" value="19" />
</widget>
